<?xml version="1.0" encoding="UTF-8"?>
<MonitoringTimerTasks>

	<Task>

		<type type = "fr.dyade.aaa.common.monitoring.FileMonitoringTimerTask" />
		<period period = "10000" />
		<taskProps objectName = "resultPath" 
							 attribute  = "monitoring2.csv" />
		<taskProps objectName = "name" 
							 attribute  = "Monitoring" />

		<attsProps objectName = "AgentServer:server=AgentServer#2,cons=Engine#2"
							 attribute  = "AverageLoad1,
														 NbWaitingMessages" />
		<attsProps objectName = "AgentServer:server=AgentServer#2,cons=Transaction"
							 attribute  = "GarbageTime,
														 GarbageCount"/>
		<attsProps objectName = "Joram#2:type=Destination,name=*"
							 attribute  = "NbMsgsReceiveSinceCreation,
 														 PendingMessageCount" />
	</Task>

</MonitoringTimerTasks>
