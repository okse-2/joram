   <extension point="org.eclipse.emf.validation.constraintProviders">
      <?gmfgen generated="true"?>
            <category id="joramcontainer" mandatory="false" name="JORAM integrity">
         <![CDATA[Checks integrity of the JORAM diagram.]]>
      </category>
      <constraintProvider cache="true">
         <package namespaceUri="http:///joram.ow2.org/ecore/joram"/>
               <constraints categories="joramcontainer">
                  <constraint id="uniqueIdRule"
            lang="OCL"
            name="JORAM unique server Id"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[JORAM.allInstances()->forAll(j | j.sid = self.sid implies j = self)]]>
            <description><![CDATA[Checks unicity of server id.]]></description>
            <message><![CDATA[Server Id (sid) must be unique.]]></message>
            <target class="joram.JORAM"/>
         </constraint>
                  <constraint id="uniqueDomainRule"
            lang="OCL"
            name="Domain unique name"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[NetworkDomain.allInstances()->forAll(n | n.name = self.name implies n = self)]]>
            <description><![CDATA[Checks unicity of domain name.]]></description>
            <message><![CDATA[Domain name must be unique.]]></message>
            <target class="joram.NetworkDomain"/>
         </constraint>
                  <constraint id="domainNameRule"
            lang="OCL"
            name="Domain name check"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[name.size() > 0]]>
            <description><![CDATA[descr TODO]]></description>
            <message><![CDATA[Domain name can't be empty]]></message>
            <target class="joram.NetworkDomain"/>
         </constraint>
                  <constraint id="portcheck"
            lang="OCL"
            name="Port used check"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.network->forAll(c1,c2 | c1 <> c2 implies c1.port <> c2.port)]]>
            <description><![CDATA[TODO]]></description>
            <message><![CDATA[Server is linked with two different domains using the same port.]]></message>
            <target class="joram.ScalAgentServer"/>
         </constraint>
                  </constraints>
      </constraintProvider>
   </extension>

